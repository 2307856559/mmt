<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lemon.shared.log.mapper.LogManager">
	<insert id="saveSiteAccessLog" parameterType="lemon.shared.log.bean.SiteAccessLog">
		insert into log_site_access(signature,timestamp,nonce,echostr,log_date)
			select #{signature},#{timestamp},#{nonce},#{echostr},now()
	</insert>
</mapper>